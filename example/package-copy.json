{
  "name": "@monchilin/react-native-dropdown-example",
  "description": "Example app for @monchilin/react-native-dropdown",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start": "expo start",
    "test": "jest"
  },
  "dependencies": {
    "@monchilin/react-native-dropdown": "file:../",
    "expo": "^45.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-elements": "^4.0.0-rc.2",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.18.3",
    "@expo/webpack-config": "^0.16.24",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "~9.1.0",
    "typescript": "~4.7.3"
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "requireConfigFile": "off",
      "no-shadow": 0,
      "react-native/no-inline-styles": 0,
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  }
}
