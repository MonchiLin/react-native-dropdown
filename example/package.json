{
  "name": "@monchilin/react-native-dropdown-example",
  "description": "Example app for @monchilin/react-native-dropdown",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start": "expo start",
    "test": "jest"
  },
  "dependencies": {
    "expo": "~39.0.3",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-native": "~0.63.3",
    "react-native-elements": "^2.3.2",
    "react-native-unimodules": "~0.11.0",
    "react-native-web": "~0.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/runtime": "^7.12.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.3.0",
    "expo-cli": "^3.28.1"
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "no-shadow": 0,
      "react-native/no-inline-styles": 0,
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  }
}
